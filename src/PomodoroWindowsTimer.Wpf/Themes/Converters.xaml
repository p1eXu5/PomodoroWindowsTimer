<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:conv="clr-namespace:PomodoroWindowsTimer.Wpf.Converters"
                    xmlns:local="clr-namespace:PomodoroWindowsTimer.Wpf"
                    >
    <conv:ActualSizeToCenterPointConverter x:Key="{x:Static local:PwtConverters.ActualSizeToCenterPointKey}" />
    <conv:ActualSizeToOuterTopRightPointConverter x:Key="{x:Static local:PwtConverters.ActualSizeToOuterTopRightPointKey}" />
    <conv:ActualWidthToCenterPointConverter x:Key="{x:Static local:PwtConverters.ActualWidthToCenterPointKey}" />
    <conv:ActualWidthToEndPointConverter x:Key="{x:Static local:PwtConverters.ActualWidthToEndPointKey}" />
    <conv:ActualWidthToOriginPointConverter x:Key="{x:Static local:PwtConverters.ActualWidthToOriginPointKey}" />
    <conv:DividerConverter x:Key="{x:Static local:PwtConverters.DividerKey}" />
    <conv:MaxDividerConverter x:Key="{x:Static local:PwtConverters.MaxDividerKey}" />
    <conv:ScaleConverter x:Key="{x:Static local:PwtConverters.ScaleKey}" />
    <conv:MultiWidthToGradientStopsConverter x:Key="{x:Static local:PwtConverters.MultiWidthToGradientStopsKey}" />
</ResourceDictionary>