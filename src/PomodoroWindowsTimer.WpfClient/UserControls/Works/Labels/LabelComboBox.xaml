<ComboBox x:Class="PomodoroWindowsTimer.WpfClient.UserControls.Works.Labels.LabelComboBox"
          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
          xmlns:local="clr-namespace:PomodoroWindowsTimer.WpfClient.UserControls.Works.Labels"
          xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
          d:DesignHeight="30"
          d:DesignWidth="200"
          d:Foreground="{StaticResource MaterialDesign.Brush.Primary.Dark.Foreground}"
          d:Background="{StaticResource MaterialDesign.Brush.Primary.Dark}"
          materialDesign:HintAssist.Hint="Label"
          ItemsSource="{Binding Labels}"
          mc:Ignorable="d"
          />
