<UserControl x:Class="PomodoroWindowsTimer.WpfClient.UserControls.Works.WorkEvents.WorkEventListDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PomodoroWindowsTimer.WpfClient.UserControls.Works.WorkEvents"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:DailyStatisticListModel="clr-namespace:PomodoroWindowsTimer.ElmishApp.DailyStatisticListModel;assembly=PomodoroWindowsTimer.ElmishApp"
             d:DataContext="{d:DesignInstance Type=DailyStatisticListModel:Bindings, IsDesignTimeCreatable=False}"
             d:DesignHeight="450"
             d:DesignWidth="800"
             Loaded="SetDataContextToDailyStatistics"
             mc:Ignorable="d"
             >
    <DockPanel>
        <Button DockPanel.Dock="Bottom"
                Content="CLOSE"
                Style="{StaticResource PomodoroCancelDialogButton}"
                />
        <!--  DataContext is set in code behind, see SetDataContextToDailyStatistics  -->
        <local:WorkEventList x:Name="m_WorkEventList"
                             DataContext="{Binding WorkEventListDialog}"
                             />
    </DockPanel>
</UserControl>
